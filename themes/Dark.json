{
  "type": "dark",
  "colors": {
    "focusBorder": "#3c7eeb42",
    "input.background": "#ffffff09",
    "input.border": "#b0b1d80f",
    "list.focusBackground": "#4c5ead58",
    "list.activeSelectionBackground": "#4c5ead58",
    "list.inactiveSelectionBackground": "#4c5ead58",
    "list.inactiveSelectionForeground": "#ffffff",
    "list.hoverBackground": "#b0b1d813",
    "scrollbar.shadow": "#00000067",
    "scrollbarSlider.background": "#ffffff09",
    "scrollbarSlider.hoverBackground": "#ffffff18",
    "scrollbarSlider.activeBackground": "#ffffff13",
    "editor.background": "#14161a",
    "editor.foreground": "#8599b3",
    "editor.selectionBackground": "#333470",
    "editor.lineHighlightBackground": "#ccccff07",
    "editorCursor.foreground": "#FF4A98",
    "editorWhitespace.foreground": "#282f40",
    "editorLineNumber.foreground": "#ccccff31",
    "tab.activeBackground": "#15171b",
    "tab.inactiveBackground": "#0c0e12",
    "tab.border": "#ffffff10",
    "tab.inactiveForeground": "#ffffff42",
    "editorGroupHeader.tabsBackground": "#0c0e12",
    "editorGroupHeader.tabsBorder": "#14161a",
    "editorGroup.border": "#ffffff12",
    "activityBar.background": "#0c0e12",
    "activityBar.foreground": "#ffffff76",
    "sideBar.background": "#0c0e12",
    "sideBar.foreground": "#ffffffcc",
    "sideBar.border": "#ffffff13",
    "sideBarTitle.foreground": "#ffffff42",
    "sideBarSectionHeader.background": "#0c0e12",
    "sideBarSectionHeader.foreground": "#ffffff94",
    "titleBar.inactiveForeground": "#ffffff67",
    "titleBar.activeBackground": "#0c0e12",
    "titleBar.inactiveBackground": "#0c0e12"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#8599b3ff",
        "background": "#14161aff"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#465173"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#0AFAFA"
      }
    },
    {
      "name": "Number/Await",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#ff944a"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language, constant.character, constant.other",
      "settings": {
        "foreground": "#0AFAFA"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "variable.other.readwrite",
      "settings": {
        "foreground": "#EFEFEF"
      }
    },
    {
      "name": "Keywords",
      "scope":
        "keyword.operator.comparison.js, keyword.control.flow, keyword.control.trycatch, keyword.control.conditional, keyword.operator.accessor",
      "settings": {
        "foreground": "#C678DD",
        "fontStyle": "italic"
      }
    },
    {
      "name":
        "Object Functions, Object Property Curly, Arrow Function/Storage Type, Variable",
      "scope":
        "meta.function.arrow.js, storage.type, meta.group.braces.curly.js, meta.function-call.without-arguments.js",
      "settings": {
        "foreground": "#FF4A98"
      }
    },
    {
      "name": "Object Module/Process, Support Type",
      "scope":
        "support.type, support.type.object, keyword.operator.accessor",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#ffee4a",
        "fontStyle": "italic, underline"
      }
    },
    {
      "name": "Variable Functions",
      "scope": "variable, variable.function.js",
      "settings": {
        "foreground": "#45B8FF"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic ",
        "foreground": "#FF4A98"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#E06C75"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FF4A98"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#4a7bff"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.type, support.class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#45B8FF"
      }
    },
    {
      "name": "Variable Other",
      "scope": "variable.other",
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "name": "Language",
      "scope": "variable.language, meta.class variable.other.readwrite",
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#FF3392",
        "fontStyle": "",
        "foreground": "#F1ADD2"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#615FFF",
        "foreground": "#B2AFF0"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    }
  ]
}
